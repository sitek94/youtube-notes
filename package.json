{
  "name": "youtube-notes",
  "description": "A Chrome Extension for taking quick notes while watching a Youtube video",
  "version": "1.0.0",
  "scripts": {
    "dev": "NODE_ENV=development webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "zip": "bestzip dist/${npm_package_name}_${npm_package_version}.zip dist/*",
    "release": "release-it"
  },
  "dependencies": {
    "easymde": "^2.15.0"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.1.1",
    "bestzip": "^2.2.0",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "parcel": "^2.1.1",
    "release-it": "^14.11.8",
    "to-string-loader": "^1.2.0",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-ext-reloader": "^1.1.8",
    "webpack-extension-reloader": "^1.1.4",
    "webpack-merge": "^5.8.0"
  }
}
